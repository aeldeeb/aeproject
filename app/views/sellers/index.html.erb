<h1>Listing sellers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Phone</th>
    <th>Username</th>
    <th>Password</th>
    <th>City</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sellers.each do |seller| %>
  <tr>
    <td><%= seller.name %></td>
    <td><%= seller.phone %></td>
    <td><%= seller.username %></td>
    <td><%= seller.password %></td>
    <td><%= seller.city %></td>
    <td><%= seller.email %></td>
    <td><%= link_to 'Show', seller %></td>
    <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
    <td><%= link_to 'Destroy', seller, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Seller', new_seller_path %>
